class Solution {
    int maxSubarraySum(int[] arr) {
       int n=arr.length;
       int currsum=0;
       int maxsum=Integer.MIN_VALUE;
       for(int i=0;i<n;i++){
           currsum+=arr[i];
           if(currsum>maxsum){
               maxsum=currsum;
           }
           if(currsum<0){
               currsum=0;
           }
       }
       return maxsum;
    }
}
