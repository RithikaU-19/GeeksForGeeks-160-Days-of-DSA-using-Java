class Solution {
    void pushZerosToEnd(int[] arr) {
        int n=arr.length;
        int rr=0;
        for(int i=0;i<n;i++){
            if(arr[i]!=0){
                arr[rr]=arr[i];
                rr++;
            }
        }
            while(rr<n){
                arr[rr]=0;
                rr++;
            }
    }
}
